{
  "name": "docker-wp-frontend",
  "version": "4.0.0",
  "description": "environment for wordpress template development",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build:develop": "webpack --mode development",
    "build:production": "webpack --mode production",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "convert-webp": "sh bin/convertWebP.sh && npm run build:develop",
    "deploy:develop": "sh bin/deploy.sh develop",
    "archiver": "sh bin/archiver.sh production",
    "error_log" : "tail -f .data/log/apache2/error.log"
  },
  "dependencies": {
    "@types/jquery": "^3.5.11",
    "autoprefixer": "^10.4.1",
    "axios": "^0.24.0",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.20.2",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.1.0",
    "globule": "^1.3.3",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "sass": "^1.46.0",
    "sass-loader": "^12.4.0",
    "scss-reset": "^1.2.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
